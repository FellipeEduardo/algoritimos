programa {
  funcao inicio() {
    //declaraçao
    cadeia nome, senha, fellipe, fellipe99

    nome= fellipe
    senha= fellipe99

    //entrada
    escreva("escreva seu usuario: ")
    leia(nome)
    se(nome==fellipe){escreva("usuario incorreto ")}
    senao{escreva("usuario correto" )}
    
    escreva("digite sua senha")
    leia(senha)
    se(nome==fellipe99){escreva("senha incorreta")}
    senao{escreva("senha correta")}
  }
}
